{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
        "react/no-array-index-key": false,
        "react/jsx-filename-extension": false,
        "prettier/prettier": [
            "error"
        ],
        "strict": 0
    }
}